	<xsd:restriction base="xsd:gYear">
		<xsd:pattern value="[0-9]{4}"/>
	</xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="yyyymmdd_date_Content">
	<xsd:restriction base="xsd:string">
		<xsd:pattern value="[0-9]{0,4}-{0,1}[0-9]{0,2}-{0,1}[0-9]{0,2}"/>
	</xsd:restriction>
</xsd:simpleType>

	<xsd:simpleType name="number_Content_0_14">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{0,14}|[0-9.0-9]{1,15}|[\-0-9]{0,15}|[\-0-9.0-9]{1,16}"/>
		</xsd:restriction>
