	</xsd:restriction>
</xsd:simpleType>

<xsd:simpleType name="char_1_256_Content">
	<xsd:restriction base="xsd:string">
		<xsd:pattern value=".{1,256}"/>
	</xsd:restriction>
</xsd:simpleType>

