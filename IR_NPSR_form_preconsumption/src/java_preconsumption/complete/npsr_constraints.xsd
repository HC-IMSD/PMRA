	<xsd:restriction base="xsd:string">
		<xsd:pattern value=".{0,10}"/>
	</xsd:restriction>
</xsd:simpleType>

	<xsd:simpleType name="number_Content_1_14">
		<xsd:restriction base="xsd:string">
