/*
* Deletes the specified subform report
*@param toDeletePage- the subform report instance to delete
*
*/

function deleteSubReport(toDeletePage){
	var fieldFormNode=cFormNodeManager.getFormNodeForSom(toDeletePage.somExpression);
	var instanceManager=fieldFormNode.getInstanceManager();
	if(!instanceManager.count&gt;instanceManager.min) return;
	
		//change to previous page
		setPreviousPage();
		if(instanceManager.count===1){
			//need to deselect the form type
			cGeneralHandler.deselectSubReport(toDeletePage);
		}
		switch(toDeletePage.name){
		
			case (REPORT_BASE+"4"): //environment
				cEnvironHandler.deleteAllAttachments(toDeletePage);
			break;
			
			case (REPORT_BASE+"7"):
				cSStudyHandler.deleteAllAttachments(toDeletePage);
			break; 	
		}
		
		cGeneralUtil.removeSpecificSubformInstance(instanceManager,toDeletePage);
}///fn

