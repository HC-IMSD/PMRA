*
*
*/
function setSignAndDateState(){
	
	if( !cGeneralHandler.isNewReport()){
		SIGNATURE_TXT_FN.getNodeInstance().rawValue="";
		SIGNATURE_DTE_FN.getNodeInstance().rawValue="";
	}
}



/*
* Sets the xml indicator if the form was electronically signed
* Legacy functionality
*/
function _setElectronicSignedState(acroDoc){

 	var signIndTxtInstance=SIG_ESIGN_TXT_FN.getNodeInstance();
	var eSignFldInstance=ESIG_SIG_FN.getNodeInstance();
	 signIndTxtInstance.rawValue=NO;
	var acroSignFld=acroDoc.getField(eSignFldInstance.somExpression.substr(14)); 

	if(acroSignFld.type=="signature"){
		cLog.trace("Inside "+acroSignFld.signatureInfo().date);
		if(acroSignFld.signatureInfo().date){
	 		 signIndTxtInstance.rawValue=YES;
		}else{
	 		 signIndTxtInstance.rawValue=NO;
	 	}
	}//endif

}//end fn




