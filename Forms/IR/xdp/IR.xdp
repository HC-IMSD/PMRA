		var buffer = this.className + "[";
		
	    if (this.formNode != null)
	    {
	    	buffer += "formNode.somExpression= " + this.formNode.somExpression;
	    }
	    else
	    {
	    	buffer += "formNode= null";
	    }
		buffer += "]";
		return buffer;
	}

	//===== PRIVATE ============================================================


	
	this._checkUpdateIRNum = function(fField){
	
		if(fField.isNull) return true;
		var fieldValue=fField.rawValue;
		

		if(fieldValue.length&lt;8) return ERROR_LENGTH;	
		
		var arraySub = fieldValue.split("-");
		var toEval;
		var subYear;
		var subNum;
		if(arraySub.length===1){
			
			subYear=fieldValue.substring(0,4);
			subNum=fieldValue.substring(4,fieldValue.length);
		}else if(arraySub.length===2){
			
			subYear=arraySub[0];
			subNum=arraySub[1];
		}else{
		
			return ERROR_NOTNUM;
		}
		if(arraySub.length===2 &amp;&amp; fieldValue.length&lt;9){
		
			 return ERROR_LENGTH;	
		}	
		if(isNaN(subYear)|| isNaN(subNum)){
			return ERROR_NOTNUM;
		}
		if(new Number(subYear) &lt; 2007){
				return ERROR_2007;
		}
		if(subYear.length!=4){
			
			return ERROR_2007;
		}
		
		if(arraySub.length===1){	
				
			var updateNum=subYear+"-"+subNum;	
			fField.rawValue=updateNum;
						
		}	
		return true;
	
	}//end fn


	

}//end class




</script>
