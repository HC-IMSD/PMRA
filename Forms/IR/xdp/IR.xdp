	var systemInstance=ANIMAL_SYSTEM_DDL_FN.getNodeInstance(subFormInstance);
	var symptomInstance=ANIMAL_DESCRIPT_DDL_FN.getNodeInstance(subFormInstance);
	var toolTipMsg="";
	if(systemInstance.rawValue===""|| systemInstance.isNull){
		toolTipMsg=cResourceManager.get("ttSystemUnselected");	
	}else{		
		var index=cGeneralUtil.getDDLSaveValueIndex(systemInstance,systemInstance.rawValue);
		toolTipMsg= cResourceManager.get("ttAnimalSymptom")+systemInstance.getDisplayItem(index);
	}
		cUiUtil.setToolTip(symptomInstance,toolTipMsg);
	
}//end fn

function _updateSymptomState(symptomSFInstance){
	
	var animalSymptomDescInstance= ANIMAL_DESCRIPT_DDL_FN.getNodeInstance(symptomSFInstance);
	var symptomOtherInstance=SYMPTOM_OTH_TXT_FN.getNodeInstance(symptomSFInstance);
	
	if(animalSymptomDescInstance.isNull){
		cGeneralUtil.hideAndClearNodeInstance(symptomOtherInstance);	
	}
	else if(animalSymptomDescInstance.rawValue===OTHER ||(animalSymptomDescInstance.rawValue).indexOf("_OTH")&gt;-1){		
		//make the other field visible
		cUiUtil.show(symptomOtherInstance);
		cUiUtil.setFocus(symptomOtherInstance);
		
	}else{
		//hide and clear the field
		cGeneralUtil.hideAndClearNodeInstance(symptomOtherInstance);	
		cUiUtil.setFocus(ADD_SYMP_BTN_FN.getNodeInstance(symptomSFInstance));
	}
	//updates tooltips as appropriate
	ANIMAL_SYMPTOM_SF_FN.updateUI(symptomSFInstance);
}

