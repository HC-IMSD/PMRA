/*
* Initializes the current page and index
* If the saved values are null defaults to the first page
*/
function initializeCurrentPage(){
	var index=cSignaturePageHandler.getSavedPageIndex();
	var page=cSignaturePageHandler.getSavedPageName();
	
	if(!index) index="0";
	if(!page) page="sfPage1";
	var query=page+"["+index+"]";
	_currpage=xfa.form.IRep.resolveNode(query);
	//set the visible Page
	for(var i=1;i&lt;=MAX_REPORTS;i++){
		var query=REPORT_BASE+i+"[*]";
		pagesList=xfa.form.nodes.item(0).resolveNodes(query);	
		for(var j=0;j&lt;pagesList.length;j++){
			var onePage=pagesList.item(j);
			if(_currpage!==onePage){
				cUiUtil.hide(onePage);	
			}else{
				cUiUtil.show(onePage);
			}//endif
		}//endfor
	}//endfor		
}//end fn 

/*
* Gets the pointer to the current page 
* @returns an adobe node representing the current page
*/
function getCurrPage(){

	return _currpage;
}//end fn


/*
* Sets the pointer to the current page
* This should be the page that is visible
* @param value - an instance of an adobe node. Should be the base page node.
*/
function setCurrPage(value){

	_currpage=value;
}//end fn

/*
* Goes to the next valid page in the sequence
* Defaults to the first page
*/
function setNextPage(){

	if(getCurrPage().name==="sfPage1"){
		cGeneralHandler.createSubForms();
	}
	
	var pgeFormNode=cFormNodeManager.getFormNodeForSom(getCurrPage().somExpression);	
	var pageList=pgeFormNode.getNodeInstances();
	var nextPagePtr=null;
	var currentPage=getCurrPage();
	if(pageList.length&gt;1 &amp;&amp; currentPage.index&lt;(pageList.length-1)){
		//get the next page index
		nextPagePtr=pageList.item(currentPage.index+1);	

	}else{
		//find the next relevant page
		var startIndex=parsePageID(getCurrPage().name)+1;
		if(startIndex&gt;MAX_REPORTS) startIndex=1;
		//get the first instance of an existing report
		for(var i=startIndex; i&lt;=MAX_REPORTS;i++){
			var query=REPORT_BASE+i+"[0]";
			nextPagePtr=xfa.form.nodes.item(0).resolveNode(query);
			if(nextPagePtr)break;
		}
	}
	cUiUtil.hide(currentPage);
	cUiUtil.show(nextPagePtr);
	setCurrPage(nextPagePtr);	
}

/*
* Goes to the previous valid page that exists in the form
* Defaults to the first page
*/
function setPreviousPage(){
	var pgeFormNode=cFormNodeManager.getFormNodeForSom(getCurrPage().somExpression);	
	var pageList=pgeFormNode.getNodeInstances();
	var nextPagePtr=null;
	
	if(getCurrPage().index&gt;0){
		//get the next page index
		nextPagePtr=pageList.item(getCurrPage().index-1);	
	}else{
		//find the next relevant page
		var startIndex=parsePageID(getCurrPage().name)-1;
		if(startIndex&lt;1) startIndex=1;
		//get the first instance of an existing report
		for(var i=startIndex; i&gt;0;i--){
			var query=REPORT_BASE+i+"[0]";
			nextPagePtr=xfa.form.IRep.resolveNode(query);
			if(nextPagePtr)break;
		}
	}
	cUiUtil.hide(getCurrPage());
	cUiUtil.show(nextPagePtr);
	setCurrPage(nextPagePtr);	
}//end fn

/*
* Parses the numerical value of the subform.
*Used as an identifier for the type of subreport
*Assumes the subform name is 6 characters followed by a number
*@param value- the string name to identify
*/

function parsePageID(value){

	var index=Number(value.substr(6));

	return(parseInt(index));
}//end fn



/*
* Sets the status update radio button state
*
*/
function setStatusUpdateState(){
	if(cGeneralHandler.hasReportsBeenCreated()){
		cGeneralHandler.lockStatusUpdateChoice(false);
	}else{
		cGeneralHandler. lockStatusUpdateChoice(true);
	}

}

/*
* For a new report, on open restore all the fields that should be readonly
*
*/
