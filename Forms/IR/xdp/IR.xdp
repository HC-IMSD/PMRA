/*
* Updates the state of an unknown checkbox and it's peers. Assumes that all the children in the provided
* subform are relevant to the unknown checkbox state
* @param  parentSubformInstance - the subform containing all the controls to lock or unlock.
* @param unknownChkInstance - the unknown checkbox to check. Checks for Yes or No
*/
function updateUnknownState(parentSubformInstance,unknownChkInstance){
 
	var parentSubform_FN=cFormNodeManager.getFormNodeForSom(parentSubformInstance.somExpression);	
	var unknownChk_FN=cFormNodeManager.getFormNodeForSom(unknownChkInstance.somExpression);		
	var isUnknown=false;
	if(unknownChkInstance.rawValue===YES){
		//reset all the data
		isUnknown=true;
		xfa.host.resetData(parentSubformInstance.somExpression);
		//lock the subform
		parentSubform_FN.lock(parentSubformInstance);
		//unlock the unknown checkbox
		unknownChk_FN.unLock(unknownChkInstance);
		//set the value of the unknown checkbox back to yes
		unknownChkInstance.rawValue=YES;	
	}else{
		parentSubform_FN.unLock(parentSubformInstance);
		isUnknown=false;
	} 	
	return isUnknown;
}//end fn
 

/*
* Updates the state of the units instance based on the other units being null. If not null locks
* and clears data from the units control
* @param  unitsInstance- the adobe instance of the control to update the editable state
* @param otherUnitsInstance - the adobe instance of the control to check for a value
*/
function _updateOtherUnits(unitsInstance,otherUnitsInstance){

	var units_FN=cFormNodeManager.getFormNodeForSom(unitsInstance.somExpression);		
	
	if(otherUnitsInstance.isNull){
		units_FN.unLock(unitsInstance);	
	}else{
		units_FN.lock(unitsInstance);
		unitsInstance.rawValue="";
	}

}//end fn



/*
* Updates the state of the form based on the subreport selections
* 
* 
*/ 
function _updateSubformChoiceState(){
 
	var humanChoiceInstance=			HUMANREP_CHK_FN.getNodeInstance();
	var animalChoiceInstance=			DOMESTICREP_CHK_FN.getNodeInstance();
	var environChoiceInstance=			ENVIRONREP_CHK_FN.getNodeInstance();
	var scientReportChoiceInstance=		SCIENTREP_CHK_FN.getNodeInstance();
	var packageReportChoiceInstance=	PACKAGEREP_CHK_FN.getNodeInstance();
	var residuesReportChoiceInstance=	RESIDUEREP_CHK_FN.getNodeInstance();
	
	//subforms to show/hide. Unfortunately, historical implementation 
	// that needs to be followed
	var dateLocSfInstance=				DATELOC_SF_FN.getNodeInstance();
	var applChoiceSfInstance=			APPLCHOICE_SF_FN.getNodeInstance();
	var regInfoSFInstance=				REGINFO_SF_FN.getNodeInstance();
	
	
	if(humanChoiceInstance.rawValue===YES ||animalChoiceInstance.rawValue===YES ||environChoiceInstance.rawValue===YES
		||packageReportChoiceInstance.rawValue===YES ){
		//set the scientfic report to locked
		scientReportChoiceInstance.rawValue=NO;
		SCIENTREP_CHK_FN.lock(scientReportChoiceInstance);
		RESIDUEREP_CHK_FN.lock(residuesReportChoiceInstance);
		//show the appropriate UI
		cUiUtil.show(dateLocSfInstance)
		cUiUtil.show(applChoiceSfInstance);
					
	}else if(scientReportChoiceInstance.rawValue===YES){
		SCIENTREP_CHK_FN.unLock(scientReportChoiceInstance);
		HUMANREP_CHK_FN.lock(humanChoiceInstance);
		DOMESTICREP_CHK_FN.lock(animalChoiceInstance);
		ENVIRONREP_CHK_FN.lock(environChoiceInstance);
		PACKAGEREP_CHK_FN.lock(packageReportChoiceInstance);
		RESIDUEREP_CHK_FN.lock(residuesReportChoiceInstance);	
		//hide these questions
		cGeneralUtil.hideAndClearNodeInstance(dateLocSfInstance);
		cGeneralUtil.hideAndClearNodeInstance(applChoiceSfInstance);
		updateUnknownState(OBSERVED_SF_FN.getNodeInstance(),OBSERVEDUNK_CHK_FN.getNodeInstance());
		
	}else if(residuesReportChoiceInstance.rawValue===YES){
		RESIDUEREP_CHK_FN.unLock(residuesReportChoiceInstance);
		HUMANREP_CHK_FN.lock(humanChoiceInstance);
		DOMESTICREP_CHK_FN.lock(animalChoiceInstance);
		ENVIRONREP_CHK_FN.lock(environChoiceInstance);
		PACKAGEREP_CHK_FN.lock(packageReportChoiceInstance);
		cUiUtil.show(dateLocSfInstance);
		cUiUtil.show(applChoiceSfInstance);	
	}else{
		//reset everything
		SCIENTREP_CHK_FN.unLock(scientReportChoiceInstance);
		HUMANREP_CHK_FN.unLock(humanChoiceInstance);
		DOMESTICREP_CHK_FN.unLock(animalChoiceInstance);
		ENVIRONREP_CHK_FN.unLock(environChoiceInstance);
		PACKAGEREP_CHK_FN.unLock(packageReportChoiceInstance);
		RESIDUEREP_CHK_FN.unLock(residuesReportChoiceInstance);
		
		cGeneralUtil.hideAndClearNodeInstance(dateLocSfInstance);
		cGeneralUtil.hideAndClearNodeInstance(applChoiceSfInstance);
		updateUnknownState(OBSERVED_SF_FN.getNodeInstance(),OBSERVEDUNK_CHK_FN.getNodeInstance());
	}
	cPestProdHandler.updateProductDescription();
	_updateApplicationInformation();
	_updateCountyStateFromReportSelections();

 	
}//end fn


	


