	}
	
	this._getActivityUnkInstance=function(){
	
		return(this.activityUnkInstance);	
	}
	
	
	
	//this.activityUnknownInstance;
	this._setColor = function(isError){
		
		if(isError){
			var errorColor=cConfigurationManager.get("colourError");
			cUiUtil.setColour(this._getActivityTxtInstance(), errorColor);
			cUiUtil.setColour(this._getActivityUnkInstance(), errorColor);				
		}else{
			cUiUtil.resetColour(this._getActivityTxtInstance());
			cUiUtil.resetColour(this._getActivityUnkInstance());
		}		
	}
	
	

}//end class


