* @param pageName- the string representing the pageName
* @param pageIndex- the string representing the pageIndex
*/
function setCurrPageNameAndIndex(pageName,pageIndex){

	var saveValue=pageName+"!"+pageIndex;		
	PAGENAME_TXT_FN.getNodeInstance().rawValue=saveValue;
}
/*
* decodes the page name that was saved
*@returns the page name, adobe node name
*/
function getSavedPageName(){
	var savedValue=PAGENAME_TXT_FN.getNodeInstance().rawValue;
	if(savedValue){
		var nameArray=savedValue.split(SAVEPAGEDELIM);
		return(nameArray[0]);
	}
	return "";
}
/*
* Decodes the saved page index value
* @returns a string representing the index of the page index
*/
function getSavedPageIndex(){

	var savedValue=PAGENAME_TXT_FN.getNodeInstance().rawValue;
	if(savedValue){
		var nameArray=savedValue.split(SAVEPAGEDELIM);
		return(nameArray[1]);
	}
	return "";
}



