function _initializeAnimalReport(animalSFInstance){
	
	var ageSfInstance=			AGE_SF_FN.getNodeInstance(animalSFInstance);	
	var ageUnkChkInstance=		AGEUNK_CHK_FN.getNodeInstance(animalSFInstance);
	var wtSfInstance=			WT_SF_FN.getNodeInstance(animalSFInstance);	
	var wtUnkChkInstance=		WT_UNK_CHK_FN.getNodeInstance(animalSFInstance);
	

	//get the index and set it
	ANIMAL_INDEX_TXT_FN.getNodeInstance(animalSFInstance).rawValue=animalSFInstance.index;	

	//Q6
	cGeneralHandler.updateUnknownState(ageSfInstance,ageUnkChkInstance);
	cGeneralHandler.updateUnknownState(wtSfInstance,wtUnkChkInstance);

	
	_setTitle(animalSFInstance);
	_updateBreedState(animalSFInstance);
	//_hideUpdateField(animalSFInstance);
	_initializeAnimalSymptoms(animalSFInstance);
	_updateHospitalizedUIState(animalSFInstance);
	_updateExposedState(animalSFInstance);
}


function _initializeAnimalSymptoms(animalReportInstance){

	var symptomsList= ANIMAL_SYMPTOM_SF_FN.getNodeInstances(animalReportInstance);
	for(var i=0;i&lt;symptomsList.length;i++){
		var symptomsRecordInstance=symptomsList.item(i);
		var systemInstance=ANIMAL_SYSTEM_DDL_FN.getNodeInstance(symptomsRecordInstance);
		_updateSymptomList(systemInstance,symptomsRecordInstance);
		_updateSymptomState(symptomsRecordInstance);
		_setSymptomToolTip(symptomsRecordInstance);
	}
}



function _updateSymptomList(systemTypeInstance,symptomSFInstance){
	
	var systemTypeValue=systemTypeInstance.rawValue;
	var symptomInstance=ANIMAL_DESCRIPT_DDL_FN.getNodeInstance(symptomSFInstance);
	if(!systemTypeValue) return;
	
	var pickListName=cFormHandler.getSystemDropList(systemTypeValue);
	
	cUiUtil.setPickList(symptomInstance, cResourceManager.getPickList(pickListName));
	//set the tooltip
	_setSymptomToolTip(symptomSFInstance);
}


function _addSymptoms(){
	var symptomInstanceManager= ANIMAL_SYMPTOM_SF_FN.getInstanceManager();
	var newInstance=cGeneralUtil.addSubformInstance(symptomInstanceManager);
	if(newInstance){
		_updateSymptomState(newInstance);
		_setSymptomToolTip(newInstance);
	}
 	return (newInstance);
}



