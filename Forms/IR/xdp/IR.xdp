	* Private function to determine for  a give product if there are active ingredients
	*
	*
	*
	*/
	this._activeIngredientsExist = function(productInstance)
	{
	
		var presetActiveList= cPestProdHandler.PRESETACTIVE_SF_FN.getNodeInstances(productInstance);
		var userActiveList= cPestProdHandler.OTHER_ACTIVE_SF_FN.getNodeInstances(productInstance);
		if(presetActiveList.length&gt;0 || userActiveList.length&gt;0){
			return true;
		}
		return false;
	}
	
	this._getProductIDLabel = function()
	{
	
		return ( cPestProdHandler.PRODID_LBL_FN.getLabel());
	
	}
	
	this._getProductIDIndex = function(productInstance)
	{
		var prodIDInstance= cPestProdHandler.PRODUCTID_TXT_FN.getNodeInstance(productInstance);
		return(prodIDInstance.rawValue);
	}
	
	this._getAllProducts=function(){
	
		var productInstanceList= cPestProdHandler.PRODUCT_SF_FN.getNodeInstances();	
		return(productInstanceList);
	}
	

}//end class



	


