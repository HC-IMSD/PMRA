var REPORT_BASE=	cConfigManager.get("REPORT_BASE_ID");

//private variables

var _currpage="";		//pointer to the current page


//initializes form nodes
_initialize();


/*
* Performs the called actions
* @param  anEvent- the event object containing the action and Source
*
*/
function actionPerformed(anEvent)
{
	if(!isHostVersionSupported()) return;
	
	switch(anEvent.getActionCommand())
	{
		case INITIALIZE_FORM_ACTION:
				_updateFormLanguage();
				initialize();
			break;
		
		case LANGUAGE_CHANGE:
				_updateFormLanguage();
			break;
		
		case VALIDATE_ACTION:
				var currPageInstance=getCurrPage();
				var pageFormNode=cFormNodeManager.getFormNodeForSom(currPageInstance.somExpression);
				var errors=validateForm( pageFormNode,currPageInstance);
				if(!errors){
					xfa.host.messageBox(cResourceManager.get("validation_pass"),cResourceManager.get("validation_title"));
				}
			break;
		
		case CHECK_NUMBER147_ACTION:
		
				cNumberFormatUtil.digitTestYX(anEvent.getValue(),14,7);	
			break;
			
		case CHECK_NUMBER_10_ACTION:
				cNumberFormatUtil.digitTestYX(anEvent.getValue(),10,0);	
			break;
			
		case CHECK_VALID_NUMBER_ACTION:
				cNumberFormatUtil.checkIfIntegerOrDecimal(anEvent.getValue());	
			break;
									
		case REMOVE_RESERVED_CHARS:
				cGeneralUtil.removeNewLine(anEvent.getValue());
			break;
						
		case VALIDATE_DELEGATE_EXIT:
			var fieldFormNode=cFormNodeManager.getFormNodeForSom(anEvent.getSource().somExpression);		
				if(fieldFormNode.hasDelegate()){				
					cGeneralUtil.validateField(fieldFormNode,anEvent.getSource());
				}	
			break;	
		
		case NEXT_PAGE_ACTION:
		var errors=validateForm();
		if(!errors){
			setNextPage();
			setPageChangeCursorPosition();
		}
		break;
		
		case ADD_PAGE_ACTION:
		var errors=validateForm();
			if(!errors){
				addSubReport();
				setPageChangeCursorPosition();
				var pageName=getCurrPage().name;
				var msgText=_getSubformTitleText(pageName)+cResourceManager.get("lbSuccessCreate");
				xfa.host.messageBox(msgText);
			}
		break;
		
		case DELETE_PAGE_ACTION:
		var currPage=getCurrPage();	
		if(_confirmDeletion(currPage)){
			deleteSubReport(currPage);
			setPageChangeCursorPosition();
			}
		break;
		
		case PREVIOUS_PAGE_ACTION:
		var errors=validateForm();
		if(!errors){
				setPreviousPage();
				setPageChangeCursorPosition();
			}
		break;
		
		case PRINT_FORM_ACTION:
			_printEntireForm();
		 break;	
		
		case  POSTPRINT_ACTION:
			 _postPrint();
		break;
		
		case  PREPRINT_ACTION:
			  _prePrint();	
			break;
		
		case PRESAVE_ACTION:
				_preSave();
				
			break;
		case POSTSAVE_ACTION:
				var currPageInstance=getCurrPage();
				if(currPageInstance.name===(REPORT_BASE+"8")){
					var pageFormNode=cFormNodeManager.getFormNodeForSom(currPageInstance.somExpression);
					var errors=validateForm( pageFormNode,currPageInstance);
				}
			break;
		default:
			throw cException.createInstance(SCRIPT_OBJECT_NAME
				  + ".actionPerformed() - unrecognized event '" 
				  + anEvent.getActionCommand() + "'.", "UnsupportedAction");
	}
}//end fn

function isHostVersionSupported(){

	if(xfa.host.version&gt;9.0){
	
		return true;
	}
	return false;
}//end fn




