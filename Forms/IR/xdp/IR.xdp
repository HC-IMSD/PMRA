/*
* Adds an active ingredient to the selected list of ingredients. Checks if the value already exists or not.
* If it already exists, does not add it to the list
*
*
*/
function addActiveToSelected(){

	var activeListInstance=getActiveListInstance(); //list of all actives
	var selectActiveInstance=getSelectedActiveListInstance();
	
	var listSelected=selectActiveInstance.resolveNode("#items"); //get the displayed values of the already selected actives
	var addToList=true;
	for (var i=0;i&lt;listSelected.nodes.length;i++){
		var result=activeListInstance.boundItem(listSelected.nodes.item(i).value); //get the save value
		if(result===activeListInstance.rawValue) //check if the value matches existin
		{
			addToList=false;
			break;
		}
	}//end for
		
	if(addToList===true){
		//get the index 
		var index=cGeneralUtil.getDDLSaveValueIndex(activeListInstance,activeListInstance.rawValue);
		if(index&gt;-1){
			var displayValue=activeListInstance.getDisplayItem(index);
			selectActiveInstance.addItem(displayValue,activeListInstance.rawValue);
		}
	}
		
}// end fn



