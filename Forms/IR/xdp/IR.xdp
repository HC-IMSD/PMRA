/*
* Performs the called actions
* @param  anEvent- the event object containing the action and Source
*
*/
function actionPerformed(anEvent)
{
	switch(anEvent.getActionCommand())
	{
		case UPDATE_UNKNOWN_GRP_ACTION:
				cGeneralHandler.updateUnknownState(anEvent.getValue(),anEvent.getSource());
			break;	
			
		case CHECKNUMBER_OR_DECIMAL:		
				cNumberFormatHandler.checkIfIntegerOrDecimal(anEvent.getValue());
			break;
			
		case UPDATE_ATTACH_REP_ACTION:
				_attachEnvReportState(anEvent.getSource(), anEvent.getValue());
			break;
			
		case ADD_UPDATE_META_ACTION:
				_addEnvUpdateReport();
			break;		
	
		case REMOVE_UPDATE_META_ACTION:
		 		var result=_removeEnvUpdateReport(anEvent.getValue());
		 		if(!result) xfa.host.messageBox(cResourceManager.get("err_OneAttach"));	
			break;
			
		case ADD_META_ACTION:
				_addEnvReport();
			break;
		case REMOVE_META_ACTION:
				var result=_removeEnvReport(anEvent.getValue());
				if(!result) xfa.host.messageBox(cResourceManager.get("err_OneAttach"));	
			break;		
		
		case SETRESULT_OTHER_ACTION:
				_setIncidentResultOther(anEvent.getSource(),anEvent.getValue());
			break;	
				case SETRESULT_SPILL_ACTION:	
				_setIncidentSpill(anEvent.getSource(),anEvent.getValue());
			break;
		
		case SETRESULT_APPLICATION_ACTION:
				_setIncidentApplication(anEvent.getSource(),anEvent.getValue());
			break;		
		
		case BUFFERZONE_STATE_ACTION:
				_setBufferZoneState(anEvent.getSource(),anEvent.getValue());
				cUiUtil.moveFocus();
			break;
				
		case SET_ORGANISM_ACTION:
				 _setOrganismType(anEvent.getSource().rawValue,anEvent.getValue());
			break;	
		
		case SET_SAMPLE_STATE_ACTION:
				_envReportState(anEvent.getSource(),anEvent.getValue());
				cUiUtil.moveFocus();
			break;	
		
		case REPORT_EXTENSION_STATE_ACTION:
				_reportExtensionState(anEvent.getSource(),anEvent.getValue());
				cUiUtil.moveFocus();
			break;
			
		case BROWSE_UPDATE_FILE_ACTION:
			
			var metaRootInstance=anEvent.getValue();
			var filenameTxtInstance=ENV_UPDATE_FILENAME_TXT_FN.getNodeInstance(metaRootInstance);
			var attachIDTxtInstance=ENV_UPDATE_ATTACHLOC_TXT_FN.getNodeInstance(metaRootInstance);
			var base_id=		cConfigManager.get("ENVIRON_UPDATE_BASE");
			 cFileAttachHandler.browseForFileAttachment(metaRootInstance,event.target,base_id,filenameTxtInstance,attachIDTxtInstance);
			break;	
		
		case REMOVE_UPDATE_FILE_ACTION:
			_removeUpdateEnvReportAttachment(anEvent.getValue());
			break;
			
			
		case BROWSE_FILE_ACTION:
			
			var metaRootInstance=anEvent.getValue();
			var filenameTxtInstance=ENV_FILENAME_TXT_FN.getNodeInstance(metaRootInstance);
			var attachIDTxtInstance=ENV_ATTACHLOC_TXT_FN.getNodeInstance(metaRootInstance);
			var base_id=		cConfigManager.get("ENVIRON_NEW_BASE");
			 cFileAttachHandler.browseForFileAttachment(metaRootInstance,event.target,base_id,filenameTxtInstance,attachIDTxtInstance);
			break;	
		
		case REMOVE_FILE_ACTION:
			_removeEnvReportAttachment(anEvent.getValue());
			break;			
							
		default:
			throw cException.createInstance(SCRIPT_OBJECT_NAME
				  + ".actionPerformed() - unrecognized event '" 
				  + anEvent.getActionCommand() + "'.", "UnsupportedAction");
	}
}//end fn


