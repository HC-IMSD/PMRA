									aValidationFailure);
		var resultOK=this._checkUpdateIRNum(aNodeInstance);
		if(resultOK) this.errorResourceAlias=resultOK;
		if (resultOK!=true){	
			var newValidationFailure = cValidationFailure.createInstance(
										"SubmissionNoDelegate",
										cResourceManager.get(
										this.errorResourceAlias));
			if (aValidationFailure != null)
