/*
* Creates a unique name for a file attachment. Searches all existing attachments
* and and finds the biggest numerical value based on 'base'
* @param doc - the Acroform doc object
* @param base- the base name to search for
* @returns - The base name appended with an index
*/

function _getUniqueCName(doc,base)
{
	var result =null;
	var exists=false;
	
	
	var tAttach= doc.dataObjects; //get all the file attachments
	var biggest=0;
	if(tAttach)
	{
		for(var i=0;i&lt;tAttach.length;i++)
		{
			if((tAttach[i].name).substr(0,base.length)==base)
			{			
				var num=_getAttachIndex(tAttach[i].name);
				if(num&gt;biggest) biggest=num;
			}
		}
	}
	return(base+"_"+(biggest+1));
	
}

/*
* Given a name with a number that is preceded with an underscore, extracts the number
*@param - the string name to examine
*@returns- the index. -1 if there is no index value
*/
function _getAttachIndex(attachName)
{
	var loc=attachName.lastIndexOf('_');
	var num=parseInt(attachName.substr(loc+1,attachName.length));
	if(isNaN(num)) num=-1;
	return(num);
}

/*
*  checks the filename for invalid characters
*	@aEvent- the filename string to check
*	@returns- true if there are invalid characters in the name
*	valid characters: '0-9','A-Z','a-z','.'and '_'
*
*/
function _checkFilename(aEvent)
{
	for(var i=0;i&lt;aEvent.length;i++)
	{
		if (!(aEvent.charCodeAt(i)==46 | aEvent.charCodeAt(i)==95 |  //46='.',95='_'
			(aEvent.charCodeAt(i)&gt;96 &amp;&amp; aEvent.charCodeAt(i)&lt;123) | (aEvent.charCodeAt(i)&gt;64 &amp;&amp; aEvent.charCodeAt(i)&lt;91)//letters
					 |(aEvent.charCodeAt(i)&gt;47 &amp;&amp; aEvent.charCodeAt(i)&lt;58)))//0-9
		{
			return(true);
		}	
	}	
return(false);
}

/************************************************************************************************
Function- attachNameDuplicate -determines if an attachment ALREADY exists with the same file name as the one 
							with the given doc_id ie checks for duplicate filenames
Parameters- doc_id - the cName of the attached file to compare for duplicate filenames
		   	doc - a pointer to the current incident report
Returns: true if the filename exists in an attachment other then doc_id
*************************************************************************************************/
function _attachNameDuplicate(doc,doc_id,dMsg)
{
	//get the doc that was just attached
	var tAttach= doc.getDataObject(doc_id);
	if(tAttach.path=== EINDEX_NAME){
		return true;
	}
	//get all the data objects and ensure the file names are unique
	var gAttach=doc.dataObjects;
	if(gAttach)
	{
		for(i=0;i&lt;gAttach.length;i++)
		{
			if(gAttach[i].path==tAttach.path &amp;&amp; gAttach[i].name!=tAttach.name)
			{
				if(dMsg){
					var message=cResourceManager.get("err_filename")+tAttach.path+cResourceManager.get("err_filenameUsed");
				 xfa.host.messageBox(message);
				}
				doc.removeDataObject(doc_id);
				return(true);
			}
		}
	}
	return(false);
}//endfn





