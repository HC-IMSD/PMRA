			break;
		case UPDATE_LOCATION_COUNTRY:
				updateCountryUIState();
			break;	
						
		case UPDATE_FORMULATION_OTHER:
			_updateOtherFormulationType();
			break;
					
		case ADDSITE_ACTION:
				_addApplicationSite();
			break;
			
		case REMOVESITE_ACTION:
				var siteDeleteResult=_deleteApplicationSite(anEvent.getValue());
				if(!siteDeleteResult){
					xfa.host.messageBox(cResourceManager.get("err_oneSite"),"",3,0);
				}else{
					// TO DO setcursor
				}		
			break;
			
		case SITECHANGED_ACTION:
				_checkForDuplicateApplicationSites(anEvent.getValue());
				_siteUsed(anEvent.getSource(),anEvent.getValue());
				cUiUtil.moveFocus();
			break;
			
		case VALIDATE_FIELD_ACTION:	
				var fieldFormNode=cFormNodeManager.getFormNodeForSom(anEvent.getSource().somExpression);		
				if(fieldFormNode.hasDelegate()){				
					cGeneralUtil.validateField(fieldFormNode,anEvent.getSource());
				}
			break;	

		case HUMAN_SELECT_ACTION:
				var result=_checkSubReportsState(anEvent.getSource(),cHumanReportHandler.getHumanReportInstanceManager());	
				if(result) _updateSubformChoiceState();
			break;
		case ANIMAL_SELECT_ACTION:
				var result=_checkSubReportsState(anEvent.getSource(), cAnimalReportHandler.getAnimalReportInstanceManager());	
				if(result) _updateSubformChoiceState();
			break;
			
		case ENVIRON_SELECT_ACTION:
				var result=_checkSubReportsState(anEvent.getSource(), cEnvironReportHandler.getEnvironReportInstanceManager());	
				if(result) _updateSubformChoiceState();	
			break;		
	
		case RESIDUE_SELECT_ACTION:
				var result=_checkSubReportsState(anEvent.getSource(), cResidueReportHandler.getResidueReportInstanceManager());	
				if(result) _updateSubformChoiceState();	
			break;		
		
		case PKG_SELECT_ACTION:
				var result=_checkSubReportsState(anEvent.getSource(), cPkgFailureHandler.getPkgFailureReportInstanceManager());	
				if(result) _updateSubformChoiceState();	
			break;
			
		case SSTUDY_SELECT_ACTION:
				var result=_checkSubReportsState(anEvent.getSource(), cSStudyHandler.getSStudyReportInstanceManager());	
				if(result) _updateSubformChoiceState();	
			break;	
		
		default:
			throw cException.createInstance(SCRIPT_OBJECT_NAME
				  + ".actionPerformed() - unrecognized event '" 
				  + anEvent.getActionCommand() + "'.", "UnsupportedAction");
	}//end switch
}//end fn



