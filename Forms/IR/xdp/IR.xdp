/*
* Performs the called actions
* @param  anEvent- the event object containing the action and Source
*
*/
function actionPerformed(anEvent)
{
	switch(anEvent.getActionCommand())
	{
		case ARE_ATTACH_UPDATEACTION:
		 		_attachingStudy(anEvent.getValue());
			break;		
		
		case ADD_UPDATE_META_ACTION:
				_addUpdateMetaReport(anEvent.getValue());	
			break;	
			
		case REMOVE_UPDATE_META_ACTION:
				var result=_removeUpdateReport(anEvent.getValue());
				if(!result) xfa.host.messageBox(cResourceManager.get("err_OneAttach"));
			break;	
		
		case BROWSE_UPDATE_FILE_ACTION:
			
			var metaRootInstance=anEvent.getValue();
			var filenameTxtInstance=UPDATEMETA_FILENAME.getNodeInstance(metaRootInstance);
			var attachIDTxtInstance=UPDATEMETA_FILELOC.getNodeInstance(metaRootInstance);
			var base_id=cConfigManager.get("SSTUDY_UPDATE_BASE");
			 cFileAttachHandler.browseForFileAttachment(metaRootInstance,event.target,base_id,filenameTxtInstance,attachIDTxtInstance);
			break;	
			
			
		case REMOVE_UPDATE_FILE_ACTION:
			
			var metaRootInstance=anEvent.getValue();
			var filenameTxtInstance=UPDATEMETA_FILENAME.getNodeInstance(metaRootInstance);
			var attachIDTxtInstance=UPDATEMETA_FILELOC.getNodeInstance(metaRootInstance);
			 cFileAttachHandler.removeAttachment(metaRootInstance,event.target,filenameTxtInstance,attachIDTxtInstance);
			break;	
			
		case BROWSE_FILE_ACTION:
			
			var metaRootInstance=anEvent.getValue();
			var filenameTxtInstance=META_FILENAME.getNodeInstance(metaRootInstance);
			var attachIDTxtInstance=META_FILELOC.getNodeInstance(metaRootInstance);
			var base_id=		cConfigManager.get("SSTUDY_BASE");
			cFileAttachHandler.browseForFileAttachment(metaRootInstance,event.target,base_id,filenameTxtInstance,attachIDTxtInstance);
		
		break;	
		
		case REMOVE_FILE_ACTION:
			
			var metaRootInstance=anEvent.getValue();
			var filenameTxtInstance=META_FILENAME.getNodeInstance(metaRootInstance);
			var attachIDTxtInstance=META_FILELOC.getNodeInstance(metaRootInstance);
			 cFileAttachHandler.removeAttachment(metaRootInstance,event.target,filenameTxtInstance,attachIDTxtInstance);
			break;	
		
		case ADD_UPDATE_DACO_ACTION:
		
			_addUpdateDaco(anEvent.getValue());
		break;
		
		case ADD_DACO_ACTION:
				_addMetaDaco(anEvent.getValue());
			break;
		
		case REMOVE_DACO_ACTION:
			_removeMetaDaco(anEvent.getValue());
			break;
			
		case REMOVE_UPDATE_DACO_ACTION:
		
			_removeUpdateDaco(anEvent.getValue());
			break;	
				
		
		case ADD_META_ACTION:
		
				_addMetaReport(anEvent.getValue());
			break;
			
		case REMOVE_META_ACTION:
		
					var result=_removeMetaReport(anEvent.getValue());
					if(!result) xfa.host.messageBox(cResourceManager.get("err_OneAttach"));	
			break;		
		
		case ISDISCONTIN_ACTION:
				_updateDiscontinuedState(anEvent.getValue());
				break;
		case COMPLETION_UNK_ACTION:		
				cGeneralHandler.updateUnknownState(anEvent.getValue(),anEvent.getSource());	
				break;	
				
		case EXTENSION_NEEDED_ACTION:
		
				_updateExtensionUIState(anEvent.getValue());
				break;
				
		case UPDATE_AUTHOR_STATE_ACTION:		
			
			_updateStudyAuthorState(anEvent.getValue())
			break;
										
		default:
			throw cException.createInstance(SCRIPT_OBJECT_NAME
				  + ".actionPerformed() - unrecognized event '" 
				  + anEvent.getActionCommand() + "'.", "UnsupportedAction");
	}
}//end fn



