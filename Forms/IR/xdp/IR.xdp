	this._isOneItemCompleted = function(aNode)
	{
		var isValid=false;
		if(aNode.presence!=="visible") return(true);	
		var childrenNodeList = aNode.nodes;
		if (childrenNodeList=== null) return(true);
		
		for(var i=0; i &lt; childrenNodeList.length; i++){
			var childNode=childrenNodeList.item(i);
			if(childNode.className == "field"){
					
				//radio buttons are the same as checkbox, so this wont work for radio butttons
				if(childNode.ui.oneOfChild.className == "checkButton"){
					this.checkBoxOverrideNode=childNode;					
				}//endif
				if(childNode.ui.oneOfChild.className == "choiceList"){
					this.dropListNode=childNode;					
				}//endif
				if(childNode.ui.oneOfChild.className == "textEdit"){
					this.fieldValueNode=childNode;					
				}//endif
				
			}//endif			
				
		}//end for
		var dropListValue=cStringUtil.trim(this.dropListNode.rawValue);
		if(this.checkBoxOverrideNode.rawValue!==NO){
			isValid=true;
		}else if(!this.fieldValueNode.isNull &amp;&amp; dropListValue){
			isValid=true;
		}
		
		return(isValid);
	}//end fn



}//end class



