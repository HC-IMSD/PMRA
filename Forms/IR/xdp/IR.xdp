	if(!pmraRegInstance.isNull){
		pmraSubInstance.mandatory="";
		epaRegInstance.mandatory="";
		pmraRegInstance.mandatory="error";
	}else if(!pmraSubInstance.isNull){
		pmraSubInstance.mandatory="error";
		epaRegInstance.mandatory="";
		pmraRegInstance.mandatory="";	
	}else if(!epaRegInstance.isNull){
		pmraSubInstance.mandatory="";
		epaRegInstance.mandatory="error";
		pmraRegInstance.mandatory="";
	}else{
		pmraSubInstance.mandatory="error";
		epaRegInstance.mandatory="error";
		pmraRegInstance.mandatory="error";
	}
	PRODUCTREG_SF_FN.updateUI(regSFInstance);
	//this resets the tooltips
	_initializeProductTooltips(productInstance);
}

/*
* Initializes the Guarantee UI. Expects an unknown checkbox, units drop list and a units text box. 
* Manages the UI state of all 3 in a generic manner (doesn't use fixed Form Nodes)
* @param guaranteeSFInstance- the parent Adobe subform of the passed elements
* @param unknownCkInstance- the adobe node that is the unknown state value. Causes all other peer elements to be locked
* @param unitsDDLFormNode - the Form Node for the fixed list of units
* @param unitsDDLInstance- an adobe node instance of the unitsDDLFormNode Form Node
* @param otherUnitsTxtInstance - the text entry box for the units. The droplist and text box are mutually exclusive
*/
function _initializeGuaranteeUI(guaranteeSFInstance,unknownChkInstance,unitsDDLFormNode,unitsDDLInstance,otherUnitsTxtInstance){

		//check if unknown. If unknown make the elements read only
		var isUnknown=cGeneralHandler.updateUnknownState(guaranteeSFInstance,unknownChkInstance);
		//if not unknown check the units
		if(isUnknown===false){
			//if  other Units are not null lock. Don't need to unlock as this is the default state
			if(!otherUnitsTxtInstance.isNull){
				//should be locked
				unitsDDLInstance.rawValue="";
				unitsDDLFormNode.lock(unitsDDLInstance);
			}//endif
		}//endif
}//end fn


function walk(oNode, func, param1)
{	
	var count = 0;	
		
	if ((oNode.className == "exclGroup" 
	|| oNode.className == "subform"  
	|| oNode.className == "subformSet" 
	|| oNode.className == "area"))
	{
		
		for (var i = 0; i &lt; oNode.nodes.length; i++)
		{
			var oChildNode = oNode.nodes.item(i);
			count += walk(oChildNode, func, param1);
		}
	}
	else if (oNode.className == "field")
	{
		
		if(func(oNode, param1))
		{
			count++;
		}
	}
	
	return count;
}

