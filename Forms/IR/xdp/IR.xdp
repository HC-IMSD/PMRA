/*
* sets the cursor to a specified node on the previous record after the record has been deleted
* @param deletedIndex- the index of the record that was deleted
* @param subformFormNode- the form Node for the record
* @param toFocusFormNode - the form Node to find an instance of and set the focus to. 
*							Must be a child of subformFormNode
* @returns true if the moveFocus() command was sent, false otherwise
*/

function setCursorAfterRecordDelete(deletedIndex,subformFormNode,toFocusFormNode){

	var subformInstances=subformFormNode.getNodeInstances();
	var toFocusFormNodeInstance=null;
	
	if(subformInstances.length===0) return false;
	
	if (deletedIndex&lt;subformInstances.length){
		 toFocusFormNodeInstance=toFocusFormNode.getNodeInstance(subformInstances.item(deletedIndex));
	}else{
		//last instance was deleted, focus on the new last instance
		 toFocusFormNodeInstance=toFocusFormNode.getNodeInstance(subformInstances.item(subformInstances.length-1));
	}
	if(toFocusFormNodeInstance){
		cUiUtil.setFocus(toFocusFormNodeInstance);
		cUiUtil.moveFocus();
		return true;
	}
	return false;
}




