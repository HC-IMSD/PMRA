var SCRIPT_OBJECT_NAME="FormHandler";

//ACTIONS
var LANGUAGE_CHANGE=					"language_change";
var VALIDATE_ACTION=					"validate_action";
var INITIALIZE_FORM_ACTION=				"initialize_form";
var CHECK_NUMBER147_ACTION=				"number147_format";
var REMOVE_RESERVED_CHARS=				"removeReserved";
var VALIDATE_DELEGATE_EXIT=				"validateDelegateExit";
var  NEXT_PAGE_ACTION=					"nextPage"
var PREVIOUS_PAGE_ACTION=				"previousPage";
var PRINT_FORM_ACTION=					"printForm";
var POSTPRINT_ACTION=					"postPrint";
var PREPRINT_ACTION=					"prePrint";
var PRESAVE_ACTION=						"preSave";
var CHECK_NUMBER_10_ACTION=				"integerTenDigit";
var CHECK_VALID_NUMBER_ACTION=			"validNumberAction";
var ADD_PAGE_ACTION=					"addReportAction";
var DELETE_PAGE_ACTION=					"deleteReportAction";
var POSTSAVE_ACTION=					"postSave";

//FORM NODES
var PAGE_LANGAUGE_FN= 			cFormNodeManager.getFormNodeForKey("pageLanguage");


//GLOBALS

var NO=				cConfigManager.get("NO");
var YES=			cConfigManager.get("YES");
