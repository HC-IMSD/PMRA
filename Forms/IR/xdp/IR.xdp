/*
* Performs the called actions
* @param  anEvent- the event object containing the action and Source
*
*/
function actionPerformed(anEvent)
{
	switch(anEvent.getActionCommand())
	{
		case SELECT_SYSTEM_ACTION:
				//value is the parent subform
				_updateSymptomList(anEvent.getSource(),anEvent.getValue());	
				_updateSymptomState(anEvent.getValue());	//if set to unknown, other will be selected
		break;
		
		case UPDATE_SYMPTOM_STATE:	
				_updateSymptomState(anEvent.getValue());
				cUiUtil.moveFocus();	
		break;
		
		case ADD_HUMAN_SYMPTOMS:	
				_addSymptoms();	
		break;
		
		case DELETE_HUMAN_SYMPTOM:
			 var result=_deleteSymptom(anEvent.getValue());
			 if(!result){
			 	xfa.host.messageBox(cResourceManager.get("err_symptomOne"),"",3,0);				
			 }
		
		break;
		
		case UPDATE_WAS_HOSP_UI_ACTION:
			_updateHospitalizedUIState(anEvent.getValue());
			cUiUtil.moveFocus();
		break;
		
		case UPDATE_UNKNOWN_GRP_ACTION:
			cGeneralHandler.updateUnknownState(anEvent.getValue(),anEvent.getSource());
		break;
		
		case EXPOSURE_CONTACT_UI_STATE:
			_updateContactWithTreatedState(anEvent.getValue());
		
		break;
		
		case POISON_EXPOSURE_UI_STATE:
			_updatePoisoningExposureUI(anEvent.getValue());
			break;	
			
		case SET_UNKNOWN_EXPOSURE_STATE:
			 _setUnknownExposure(anEvent.getValue());	
			break;
			
		case CLEAR_IF_UNKNOWN:
			cFormHandler.clearValuesOnUnknown(anEvent.getValue(), anEvent.getSource());
			break;
			
		case UPDATE_CLOTH_NONE:
				_setClothingUnknownNoneState(anEvent.getValue(), anEvent.getSource());
			break;
				
		default:
			throw cException.createInstance(SCRIPT_OBJECT_NAME
				  + ".actionPerformed() - unrecognized event '" 
				  + anEvent.getActionCommand() + "'.", "UnsupportedAction");
	}
}//end fn

