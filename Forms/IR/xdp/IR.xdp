function _setTitle(animalSubFormInstance){

	var titleInstance=ANIMAL_REPTITLE_LBL_FN.getNodeInstance(animalSubFormInstance);
	var titleCaption=cResourceManager.get("lbAnimalTitle")+(animalSubFormInstance.index+1);
	cUiUtil.setCaption( titleInstance, titleCaption);		

