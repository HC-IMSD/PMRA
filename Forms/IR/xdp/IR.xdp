/*************************************************************************************************
Function- getRowString- creates a row of eindex metadata based on the array of data passed to it
Parameters- aRow- an array of eindex metadata to write to the file
			rowIndex- the index of the metadata, written as 'e'+rowIndex
Returns: null
Created: Jan 1, 2007
Last Modified:
Creator: D. Kilty, PMRA, Health Canada
*************************************************************************************************/
function getRowString(aRow,rowIndex)
{
	var sVALUE="&lt;value&gt;";
	var eVALUE="&lt;/value&gt;";
	var sFID="&lt;field id=";
	var eFID="&lt;/field&gt;";
	var sENTRY='entry id=';
	var eENTRY='&lt;/entry&gt;';
	var sFIDREF="&lt;field idref=";

	//row tags
	var ROW_TAGS= new Array(
	'"DACO_ID"',
	'"R_CR"',
	'"FEE_APPLIES"',
	'"CROSS_REFRENCE"',
	'"PMRA_DOC_NO"',
	'"APP_NO"',
	'"CBI_APPL_IND"',
	'"APPLICANT_DER"',
	'"DM_REPORT_NO"',
	'"DM_TITLE"',
	'"DM_AUTHOR"',
	'"DM_REPORT_DATE"',
	'"DM_NO_PAGES"',
	'"DM_VOL_NO"',
	'"DM_EPA_MRID_NO"',
	'"DM_LAB_NAME"',
	'"DM_LAB_CITY"',
	'"DM_LAB_COUNTRY"',
	'"DM_LAB_REPORT_NUMBER"',
	'"DM_GLP_GEP"',
	'"DM_PUBLISHED"',
	'"DOCUMENT_GROUP"',
	'"DM_COMMENTS"',
	'"DM_EFILE_LOCATION"',
	'"DM_EFILE_NAME"',
	'"CHECKSUM"',
	'"UKID"',
	'"DOX_DESCRIPTION"',
	'"DM_OWNER"',
	'"DM_SOURCE"',
	'"CBI_PMRA_IND"'
	);


	var UNUSED_ROW= new Array(
	'"EFFD"',
	'"EXPD"',
	'"HIST_IND"',
	'"DAFE"',
	'"DATU"',
	'"USRU"'
	);

	//extra field names and indexes
	var dox_md='"DOX_MD';
	var dox_md_max=100;
	var us_md='"US_MD';
	var us_md_max=25;

	var xmlDataStr="";
	if(aRow)
	{
		xmlDataStr+='&lt;'+sENTRY+'"e'+rowIndex+ '"&gt;\n';
		//first item is the DACO(s), in an array form
		xmlDataStr+=sFIDREF+ROW_TAGS[0]+"&gt;\n";		
		for(var z=0;z&lt;aRow[0].length;z++)
		{			
			xmlDataStr+=sVALUE+aRow[0][z]+eVALUE+"\n";	
		}
			xmlDataStr+=eFID+"\n";
		for(var t=1;t&lt;ROW_TAGS.length;t++)
		{	
			if(aRow[t]==null) aRow[t]="";
			xmlDataStr+=sFIDREF+ROW_TAGS[t]+"&gt;\n";
			xmlDataStr+=sVALUE+aRow[t]+eVALUE+"\n";
			xmlDataStr+=eFID+"\n";
		}
		//UNUSED extra fields
		for(var j=0;j&lt;UNUSED_ROW.length;j++)
		{
			xmlDataStr+=sFIDREF+UNUSED_ROW[j]+"&gt;\n";
			xmlDataStr+=sVALUE+eVALUE+"\n";
			xmlDataStr+=eFID+"\n"
		}
		//dox_md
		for (var k=0; k&lt;16;k++)
		{
			if(!(k==4 | k==13 |k==8))
			{
				xmlDataStr+=sFIDREF+dox_md+(k+1)+'"&gt;\n';
				xmlDataStr+=sVALUE+eVALUE+"\n";
				xmlDataStr+=eFID+"\n";
			}
		}
		for (var k=26; k&lt;45;k++)
		{
			if(k!=30)
			{
			xmlDataStr+=sFIDREF+dox_md+(k+1)+'"&gt;\n';
			xmlDataStr+=sVALUE+eVALUE+"\n";
			xmlDataStr+=eFID+"\n";
			}
		}
		for (var k=50; k&lt;dox_md_max;k++)
		{
		
			xmlDataStr+=sFIDREF+dox_md+(k+1)+'"&gt;\n';
			xmlDataStr+=sVALUE+eVALUE+"\n";
			xmlDataStr+=eFID+"\n";
		}
		for (var k=0; k&lt;us_md_max;k++)
		{
			xmlDataStr+=sFIDREF+us_md+(k+1)+'"&gt;\n';
			xmlDataStr+=sVALUE+eVALUE+"\n";
			xmlDataStr+=eFID+"\n";
		}
	xmlDataStr+=eENTRY+"\n"
	}//endif
	return(xmlDataStr);
}//end fn





