/*
* Clears all the entries in the selected active ingredients list
* 
*
*/
function _clearSelectedActiveList(){

	var selectedListInstance=getSelectedActiveListInstance();
	if(selectedListInstance){
		selectedListInstance.clearItems();
		selectedListInstance.rawValue="";
	}
	
}//end fn


/*
* Removes the selected active ingredient from the list
* 
*
*
*/

function _removeSelectedActiveFromList(){
	var selectedListInstance=getSelectedActiveListInstance();
	var selectListValue= selectedListInstance.rawValue;
	// 1) get the raw value
	if(selectListValue){
		// 2) get it's index in the list
		var index=cGeneralUtil.getDDLSaveValueIndex(selectedListInstance,selectListValue);
		if(index&gt;-1){
			// 4) remove it
			selectedListInstance.deleteItem(index);
			// 5) clear the rawValue selection from the list
			selectedListInstance.rawValue="";
		}	
	}

}//end fn


function _populateActiveIngredients(){
	var selectedListInstance=getSelectedActiveListInstance();
	var itemsList=selectedListInstance.resolveNode("#items");
	var listLength=itemsList.nodes.length;

	for(var i=0;i&lt;listLength;i++){
		// Get the display value/ save value pair
		var displayValue=itemsList.nodes.item(i).value;
		var activeCode= selectedListInstance.boundItem(displayValue);
		//Create and Populate the record
		cPestProdHandler.addAndPopulateActive(getProductSource(),displayValue,activeCode);		
	}
	//Close this form and return to page one
		//cPestProdHandler.showAndSetActiveFocus(getProductSource());
		cancelSelectActives();
		cUiUtil.moveFocus();
}//end fn









