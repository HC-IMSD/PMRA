	this._isOneCheckBoxChecked = function(aNodeArray)
	{
		for (var i=0; i &lt; aNodeArray.length; i++)
		{
			if (aNodeArray[i].rawValue == YES)
			{
				return true;
			}
		}
		return false;
	}
