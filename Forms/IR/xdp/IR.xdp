/*
* Sets the state of the status update subform
* @param isUpdate- a boolean, true means the report is a status update
* @param rootSFInstance- the adobe node of the root environment report
*/
function statusUpdateSetState(isUpdate,rootSFInstance){

	var statusUpdateInstance= UPDATE_STATUS_SF_FN.getNodeInstance(rootSFInstance);
	
	if(isUpdate){
		cUiUtil.show(statusUpdateInstance);
		UPDATE_STATUS_SF_FN.unLock(statusUpdateInstance);	
	}else{	
		cGeneralUtil.hideAndClearNodeInstance(statusUpdateInstance);
	}
}//end fn

/*
* Sets the report state based on the type of report that was selected
* @param isUpdate- a boolean, true means the report is a status update
* @param rootSFInstance- the adobe node of the root environment report
*/
function setReportTypeState(isUpdate,rootSFInstance){
	var areAttachingInstance=		UPDATE_ARE_ATTACH_DDL_FN.getNodeInstance(rootSFInstance);		
	
	 if(isUpdate){
		//lock appropriate fields
		 _initializeEnvironReport(rootSFInstance);
		ENVIRON_ROOT_SF_FN.lock( rootSFInstance);
	}else{
		//unlock and restore state
		ENVIRON_ROOT_SF_FN.unLock(rootSFInstance);
		 _initializeEnvironReport(rootSFInstance);
	}
	//Unlock the appropriate fields
	//update status update 
	 _attachEnvReportState(areAttachingInstance, rootSFInstance);
	_setUpdateIsAttachingState(rootSFInstance); 
	statusUpdateSetState(isUpdate,rootSFInstance); 		
}//end


function deleteAllAttachments(envRootInstance){

	var metaList=getAllMeta(envRootInstance);
	for (var i=0;i&lt;metaList.length;i++){
			var metaRootInstance=metaList.item(i);
			var filenameTxtInstance=ENV_FILENAME_TXT_FN.getNodeInstance(metaRootInstance);
			var attachIDTxtInstance=ENV_ATTACHLOC_TXT_FN.getNodeInstance(metaRootInstance);
			 cFileAttachHandler.removeAttachment(metaRootInstance,event.target,filenameTxtInstance,attachIDTxtInstance);
	}
	
	var updateMetaList=getAllUpdateMeta(envRootInstance);
	for (var i=0;i&lt;metaList.length;i++){
			var updateMetaRootInstance=metaList.item(i);
			var filenameTxtInstance=ENV_UPDATE_FILENAME_TXT_FN.getNodeInstance(updateMetaRootInstance);
			var attachIDTxtInstance=ENV_UPDATE_ATTACHLOC_TXT_FN.getNodeInstance(updateMetaRootInstance);
			 cFileAttachHandler.removeAttachment(updateMetaRootInstance,event.target,filenameTxtInstance,attachIDTxtInstance);
	}	
	
}




