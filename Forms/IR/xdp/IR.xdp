		var productInstanceList=this._getAllProducts();
		var errorText="";
		var failureID="ProductActiveIngredient";
		for(var j=0;j&lt;productInstanceList.length;j++){
			var productInstance=productInstanceList.item(j);
			if(this._activeIngredientsExist(productInstance)===false){
			
				//give a unique failure ID. Otherwise similar errors are rolled into one
				var productIndex= this._getProductIDIndex(productInstance);
				//var failureID="ProductActiveIngredient"+productIndex;
				
				if(!errorText){
				errorText+=cResourceManager.get(this.errorResourceAlias)+this._getProductIDLabel()+" "+productIndex+"\n";
