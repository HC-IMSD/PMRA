				deleteOtherActive(anEvent.getValue(),anEvent.getSource());
			break;
		case REMOVE_PRESET_ACTIVE:
				deletePresetActive(anEvent.getValue(),anEvent.getSource());
			break;
			
		case UPDATE_REG_STATE:
				_updateRegistrationState(anEvent.getValue());	
			break;
			
		case UPDATE_REG_UNKNOWN_STATE:
				//initialize will set the state 
				_initializeRegNoInfo(anEvent.getValue());
				break;		
		default:
			throw cException.createInstance(SCRIPT_OBJECT_NAME
				  + ".actionPerformed() - unrecognized event '" 
				  + anEvent.getActionCommand() + "'.", "UnsupportedAction");
	}
}//end fn


