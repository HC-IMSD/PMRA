		//make visible
		cUiUtil.show(updateRootInstance);
		UPDATEATTACH_ROOT_SF_FN.unLock(updateRootInstance);// TODO fix so I don't need to do
		_initializeAllUpdatMeta(rootSFInstance)
			
	}else{
		var updateMetaList=getAllUpdateMeta();
		for(var i=0;i&lt;updateMetaList.length;i++){
				_removeUpdateReport(updateMetaList.item(i));
		}	
		//reset the first one
		updateMetaList=getAllUpdateMeta()
		_removeUpdateResidueReportAttachment(updateMetaList.item(0));
		
		cGeneralUtil.hideAndClearNodeInstance(updateRootInstance);
	}
}//endfn
