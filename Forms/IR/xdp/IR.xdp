function actionPerformed(anEvent)
{
	switch(anEvent.getActionCommand())
	{
		
		case UPDATE_CONC_OTHER:
				_updateConcOtherUnitState();

			 break;
			 
		case UPDATE_LIMIT_OTHER:
				_updateLimitDetectOtherUnitState();
			 break;	
			 
		case REASON_COLLECTED:
		
				_reasonSamplesCollectedState();
			break;	
			
		case UPDATE_ORG_STATE:
				_organizationState();
			break;	  
					
		case EXTENSION_STATE:
				 _setAttachmentState();
			break;	
		
		//mew	
	
		case ADD_UPDATE_META_ACTION:
				_addUpdateReport();
			break;		
	
		case REMOVE_UPDATE_META_ACTION:
		 		var result=_removeUpdateReport(anEvent.getValue());
		 		if(!result) xfa.host.messageBox(cResourceManager.get("err_OneAttach"));	
			break;
			
		case ADD_META_ACTION:
				_addReport();
			break;
		case REMOVE_META_ACTION:
				var result=_removeReport(anEvent.getValue());
				if(!result) xfa.host.messageBox(cResourceManager.get("err_OneAttach"));		
			break;							
					
		case UPDATE_UNKNOWN_STATE:
			cGeneralHandler.updateUnknownState(anEvent.getValue(),anEvent.getSource());
			break;	
			
		case BROWSE_FILE_ACTION:
			
			var metaRootInstance=anEvent.getValue();
			var filenameTxtInstance=RES_FILENAME_TXT_FN.getNodeInstance(metaRootInstance);
			var attachIDTxtInstance=RES_ATTACHLOC_TXT_FN.getNodeInstance(metaRootInstance);
			var base_id= cConfigManager.get("RESIDUE_NEW_BASE");
			 cFileAttachHandler.browseForFileAttachment(metaRootInstance,event.target,base_id,filenameTxtInstance,attachIDTxtInstance);
			break;
		case REMOVE_FILE_ACTION:
			_removeResidueReportAttachment(anEvent.getValue());
			break;
			
		case ATTACH_UPDATE_ACTION:
			_attachResReportState(anEvent.getSource(),anEvent.getValue());
			break;		
											
		default:
			throw cException.createInstance(SCRIPT_OBJECT_NAME
				  + ".actionPerformed() - unrecognized event '" 
				  + anEvent.getActionCommand() + "'.", "UnsupportedAction");
	}
}//end fn



