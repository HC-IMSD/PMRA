/*
* Resets the unknown sex of the animal
*
*
*
*/
function _clearSexUnknown(parentSFInstance, nodeInstance){

	if(nodeInstance.rawValue===YES){
		SEXUNK_CHK_FN.getNodeInstance(parentSFInstance).rawValue=NO;
	
	}

}

		

	

