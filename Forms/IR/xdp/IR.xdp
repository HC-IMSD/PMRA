				controlInstance=cEnvironHandler.getOrganismTypeInstance(currentPageInstance);		
			}else{
				controlInstance= cEnvironHandler.getStatusUpdateTxtInstance(currentPageInstance);
			}
		break;
		case(REPORT_BASE+"5"): //signature
			if(cGeneralHandler.isNewReport()){
				controlInstance=cResidueHandler.getAnalyseTxtInstance(currentPageInstance);		
			}else{
				controlInstance=cResidueHandler.getStatusUpdateTxtInstance(currentPageInstance);
			}
		break;
		case(REPORT_BASE+"6"): //signature
			if(cGeneralHandler.isNewReport()){
				controlInstance=cPkgFailureHandler.getPackageTypeInstance(currentPageInstance);		
			}else{
				controlInstance= cPkgFailureHandler.getStatusUpdateTxtInstance(currentPageInstance);
			}
		break;
		case(REPORT_BASE+"7"): //signature
			if(cGeneralHandler.isNewReport()){
				controlInstance=cSStudyHandler.getStudyAuthorTxtInstance(currentPageInstance);		
			}else{
				controlInstance= cSStudyHandler.getStatusUpdateTxtInstance(currentPageInstance);
			}
		break;
		case(REPORT_BASE+"8"): //signature
			controlInstance= cSignaturePageHandler.getDateSignInstance();
		break;
		//TODO unhard code
		case ("sfSelectActive"):
			controlInstance= cActiveSelectHandler.getSelectedActiveListInstance()	
		break;
		
	}
	if(controlInstance){
		cUiUtil.setFocus(controlInstance);
		cUiUtil.moveFocus();
	}	

