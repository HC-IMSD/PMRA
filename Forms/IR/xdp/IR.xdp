function _addDaco(toAddListBoxInstance,selectedListBoxInstance, saveToTxtInstance){

	if(toAddListBoxInstance.rawValue==="-1") return;
	
	var listSelected=selectedListBoxInstance.resolveNode("#items"); //get the displayed values of the already selected actives
	var addToList=true;
	for (var i=0;i&lt;listSelected.nodes.length;i++){
		var result=toAddListBoxInstance.boundItem(listSelected.nodes.item(i).value); //get the save value
		if(result===toAddListBoxInstance.rawValue) //check if the value matches existin
		{
			addToList=false;
			break;
		}
	}//end for
		
	if(addToList===true){
		//get the index 
		var index=cGeneralUtil.getDDLSaveValueIndex(toAddListBoxInstance,toAddListBoxInstance.rawValue);
		if(index&gt;-1){
			var displayValue=toAddListBoxInstance.getDisplayItem(index);
			var valueToAdd=displayValue+DELIM+toAddListBoxInstance.rawValue;
			if(!saveToTxtInstance.isNull){
				 valueToAdd=DELIM+valueToAdd;
			}//end if
			var saveValue="";
			if(saveToTxtInstance.isNull){
				saveValue=valueToAdd;
			}else{
				saveValue=saveToTxtInstance.rawValue+valueToAdd;
			}
			saveToTxtInstance.rawValue=saveValue;
			selectedListBoxInstance.addItem(displayValue,toAddListBoxInstance.rawValue);
		}
	}

}//end fn

function _loadDacos(savedTxtInstance,selectedDacoDdlInstance,toAddListBoxInstance){
	
	if(savedTxtInstance.isNull) return;

	var dacoListValues=savedTxtInstance.rawValue;
	var parsedArray=dacoListValues.split(DELIM);
	selectedDacoDdlInstance.clearItems();
	for(var i=0;i&lt;parsedArray.length;i+=2){
		cLog.trace("This the ith item "+parsedArray[i]);
		selectedDacoDdlInstance.addItem(parsedArray[i],parsedArray[i+1]);
	}

}//fn

function _removeDaco(selectedListBoxInstance, saveToTxtInstance){
	//var selectedListInstance=getSelectedActiveListInstance();
	var selectListValue= selectedListBoxInstance.rawValue;
	// 1) get the raw value
	if(selectListValue){
		// 2) get it's index in the list
		var index=cGeneralUtil.getDDLSaveValueIndex(selectedListBoxInstance,selectListValue);
		if(index&gt;-1){
			// 4) remove it
			selectedListBoxInstance.deleteItem(index);
			//remove it from the saved list of concatenated items
			
			// 5) clear the rawValue selection from the list
			selectedListBoxInstance.rawValue="";
		}	
		//recreate the list of items, could instead do a find and delete
		var oItems = selectedListBoxInstance.resolveNode("#items");
		var newSavedItems="";
		for(var i=0;i&lt;oItems.nodes.length;i++){
			newSavedItems+=oItems.nodes.item(i).value+"|"+selectedListBoxInstance.getSaveItem(i)+"|";
		}	
		saveToTxtInstance.rawValue=newSavedItems;	
	}

}//end fn


