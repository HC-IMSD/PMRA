*
*
*
*/

function isFinalized(){

	var signatureNameInstance=	SIGNATURE_TXT_FN.getNodeInstance();
	var signatureDateInstance=	SIGNATURE_DTE_FN.getNodeInstance();
	if(signatureNameInstance.isNull || signatureDateInstance.isNull){
		return false;
	}
	return true;
}

